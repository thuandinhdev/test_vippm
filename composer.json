{
    "name": "laravel/laravel",
    "type": "project",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-dompdf": "^0.8.7",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^5.8.0",
        "laravel/tinker": "^1.0",
        "symfony/translation": "4.3.8",
        "nicolaslopezj/searchable": "1.*",
        "nwidart/laravel-modules": "^4.0",
        "spatie/laravel-responsecache": "^6.0",
        "ext-memcached": "*",
        "stripe/stripe-php": "^7.66",
        "tymon/jwt-auth": "1.0.0-rc.4"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "fzaninotto/faker": "^1.6",
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Helpers/emailGroupHelper.php",
            "app/Helpers/helpers.php",
            "app/Helpers/userActivityHelper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-install-cmd": [
            "php artisan clear-compiled",
            "chmod -R 777 'public/'",
            "chmod -R 777 'public/uploads/'",
            "chmod -R 777 'storage/'",
            "chmod -R 777 'storage/framework/'",
            "chmod -R 777 'storage/framework/sessions/'",
            "chmod -R 777 'storage/logs/'",
            "chmod -R 777 'storage/app/'",
            "chmod -R 777 'bootstrap/cache/'",
            "chmod -R 666 'install.sql'",
            "chmod -R 777 'resources/themes/'",
            "chmod -R 777 'resources/views/'",
            "chmod -R 666 '.env'"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
